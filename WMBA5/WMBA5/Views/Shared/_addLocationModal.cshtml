<!-- Modal-->
<div class="modal fade" id="addLocationModal" tabindex="-1" role="dialog" aria-labelledby="LocationModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="LocationmyModalLabel">Add New Location</h4>
                <button type="button" class="close" data-bs-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            </div>
            <div class="modal-body">
                @{
                    var location = new WMBA5.Models.Location();
                    <partial name="_LocationCreate" model="location" />
                }
            </div>
        </div>
    </div>
</div>